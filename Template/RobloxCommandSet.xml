<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="AdventureWorksCommandSet_en-us">
    <AppName> Roblox </AppName>
    <Example> Find games </Example>

    <Command Name="searchGames">
      <Example> Find Pizza Place </Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase"> find {findText} </ListenFor>
      <ListenFor RequireAppName="BeforeOrAfterPhrase"> look up {findText} </ListenFor>
      <ListenFor RequireAppName="BeforeOrAfterPhrase"> search [for] {findText} </ListenFor>
      <ListenFor RequireAppName="ExplicitlySpecified"> search {builtin:AppName} [for] {findText} </ListenFor>
      <Feedback> Searching for games </Feedback>
      <Navigate />            
    </Command>
    
    <PhraseTopic Label="findText" Scenario="Search">
      <Subject> Games </Subject>
    </PhraseTopic>
    
  </CommandSet>

